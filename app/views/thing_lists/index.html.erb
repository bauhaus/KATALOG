<h1>Listing thing_lists</h1>

<table>
  <tr>
    <th>Title</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @thing_lists.each do |thing_list| %>
  <tr>
    <td><%= thing_list.title %></td>
    <td><%= link_to 'Show', thing_list %></td>
    <td><%= link_to 'Edit', edit_thing_list_path(thing_list) %></td>
    <td><%= link_to 'Destroy', thing_list, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Thing list', new_thing_list_path %>
