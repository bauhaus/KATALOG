<h1>Resend unlock instructions</h1>

<% form_for resource_name, resource, :url => unlock_path(resource_name) do |f| %>
  <%= f.error_messages %>

  <p><%= f.label :email %><br />
  <%= f.text_field :email %></p>

  <p><%= f.submit "Resend unlock instructions" %></p>
<% end %>

<%= render :partial => "shared/devise_links" %>